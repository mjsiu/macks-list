<!DOCTYPE html>
<html>
<head>
  <title>MacksList</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <script type="text/javascript">
    window.API_KEYS = {
      cloudinary_key: "<=%ENV['CLOUDINARY_KEY']%>",
      cloudinary_cloud: "<=% ENV['CLOUDINARY_CLOUD']%>",
      googlemaps_key: "<=% ENV['GOOGLEMAPS_KEY'] %>"
    };
  </script>
  <script
    src="https://maps.googleapis.com/maps/api/js?key=" + API_KEYS['googlemaps_key']>
  </script>
</head>
<body>
  <%= yield %>
</body>
</html>
